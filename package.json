{
  "name": "market-simulator-ui",
  "title": "Market Simulator UI",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --debug --output-pathinfo --open --config webpack.dev.js",
    "test:start": "npm run test && npm run start",
    "test": "jest --updateSnapshot --collectCoverage && exit 0",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:precommit": "eslint src --max-warnings 0",
    "static": "http-server ./build/ -p 5555"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Helen Yin helen.yin@tdsecurities.com Sandy Moon sandy.moon@tdsecurities.com",
  "contributors": [
    "Helen Yin helen.yin@tdsecurities.com",
    "Sandy Moon sandy.moon@tdsecurities.com",
    "Kiat Oboler kiat.oboler@tdsecurities.com",
    "Casey Lee <casey.lee@tdsecurities.com"
  ],
  "license": "ISC",
  "jest": {
    "testResultsProcessor": "jest-teamcity-reporter",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/css-transform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/set-up-test-framework.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/assets/",
      "<rootDir>/src/components/Grpc/gen",
      "<rootDir>/src/workers/",
      "/MockData/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 1,
        "branches": 0,
        "functions": 0,
        "lines": 1
      }
    }
  },
  "pre-commit": {
    "run": "test, lint:precommit",
    "colors": false
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
